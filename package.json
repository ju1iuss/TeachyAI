{
  "name": "teachy",
  "version": "1.0.0",
  "main": "node_modules/expo-router/entry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "clean": "rm -rf ios/build ios/Pods node_modules",
    "postinstall": "cd ios && pod install && cd ..",
    "fix:pods": "./scripts/fix-pods.sh",
    "patch:dc": "node ./scripts/patch-double-conversion.js",
    "prepare:ios": "./scripts/prepare-ios-build.sh",
    "build:ios": "eas build --platform ios --profile development",
    "build:android": "eas build --platform android --profile development",
    "build:ios:prod": "./scripts/build-deploy-ios.sh",
    "build:testflight": "eas build --platform ios --profile testflight --non-interactive",
    "deploy:testflight": "eas submit -p ios --latest",
    "check:launch": "node ./scripts/check-launch-issues.js"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.7.8",
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-navigation/native": "^6.1.9",
    "@supabase/supabase-js": "^2.49.1",
    "eas-cli": "^16.1.0",
    "expo": "~52.0.46",
    "expo-av": "^15.0.2",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "^5.0.20",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.19",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.0",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-track-player": "^4.1.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@expo/ngrok": "^4.1.0",
    "@react-native-community/cli": "^18.0.0",
    "@types/react": "~18.3.12"
  },
  "private": true
}
